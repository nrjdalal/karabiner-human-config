{
  "caps": { "t": "hyper", "a": "100 caps" },
  "fn": {
    "_self": { "t": "fn", "a": "cmd tab" }
  },
  "rcmd": {
    "_self": "rcmd",
    // chat and social
    "d": "$ open \"https://discord.com/channels/@me\"",
    "k": "$ open \"https://www.instagram.com\"",
    "w": "$ open \"https://web.whatsapp.com\"",
    "x": "$ open \"https://x.com/home\"",
    // github links
    "a": "$ open \"https://github.com/nrjdalal/awesome-templates\"",
    "c": "$ open \"https://github.com/copilot\"",
    "g": "$ open \"https://github.com/nrjdalal?tab=repositories\"",
    "s": "$ open \"https://github.com/search?type=repositories&s=stars&o=desc\"",
    // productivity
    "m": "$ open \"https://mail.google.com/mail/u/0/#inbox\"",
    // streaming services
    "h": "$ open \"https://www.hotstar.com/in/home\"",
    "n": "$ open \"https://www.netflix.com/browse\"",
    "p": "$ open \"https://www.primevideo.com\"",
    "y": "$ open \"https://www.youtube.com\""
  },
  "ropt": {
    "_self": "ropt",
    "g": "$ open -a \"Google Chrome\"",
    "q": "$ open -a \"qBittorrent\"",
    "v": "$ open -a \"Visual Studio Code\""
  },
  "Visual Studio Code": {
    "fn 1": "cmd shift e",
    "fn 2": "ctrl shift g",
    "fn f": "cmd fn f2",
    "fn q": "cmd b",
    "fn tab": "cmd shift p",
    "fn tilde": "ctrl tilde",
    "fn x": "cmd shift x"
  }
}
